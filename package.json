{
  "name": "vel-extension",
  "displayName": "vel-extension",
  "description": "vel-description",
  "publisher": "vel-extension",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other" 
  ],
  "activationEvents": [
    "onView:chatgpt.chatView"
  ],
  "main": "./extension.js",
  "contributes": { 
    "viewsContainers": {
      "activitybar": [
        {
          "id": "chatgpt",
          "title": "Vel-Extension", 
          "icon": "images/icon.png"
        }
      ]
    },
    "views": {
      "chatgpt": [
        {
          "type": "webview",
          "id": "chatgpt.chatView",
          "name": "ChatGPT"
        }
      ]
    },
    "commands": [{
      "command": "vel-extension.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}